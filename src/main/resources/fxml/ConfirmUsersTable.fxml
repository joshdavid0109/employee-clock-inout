<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.helloworld.ConfirmUsersController">
    <children>
        <Label text="Employee List" />
        <TableView fx:id="employeeTable" prefHeight="360.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="usernameColumn" prefWidth="599.0" text="Usernames" />
            </columns>
        </TableView>
        <HBox alignment="CENTER_RIGHT" spacing="10.0">
            <Button fx:id="acceptButton" mnemonicParsing="false" onAction="#acceptUser" text="Accept" />
            <Button fx:id="rejectButton" mnemonicParsing="false" onAction="#rejectUser" text="Reject" />
        </HBox>
    </children>
</VBox>
